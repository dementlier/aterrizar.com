<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="ar.edu.unq.epers.aterrizar.models.Flight" table="vuelos">

		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<bag name="sections" cascade="all" lazy="false">
			<key column="section_id" />
			<one-to-many class="ar.edu.unq.epers.aterrizar.models.Section" />
		</bag>

	</class>
</hibernate-mapping>